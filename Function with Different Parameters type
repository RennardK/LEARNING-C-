// Online C++ compiler to run C++ program online
#include <iostream>
#include <string>

using namespace std;
void myProfile(string name, int age, bool employed, double height){
    cout << "Name: "<< name << endl;
    cout << "Age: "<< age << endl;
    cout << "Employed: "<< employed << endl;
    cout << "Height: "<< height << endl;
}

int main(){
    myProfile("John Doe", 23, true, 5.43);
    
    return 0;
}
