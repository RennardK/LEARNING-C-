// Online C++ compiler to run C++ program online
#include <iostream>
#include <string>
using namespace std;

void myFirstFunction(); // function declaration happening above main()

int main() {
    myFirstFunction();
    myFirstFunction();
    return 0;
}

void myFirstFunction(){ // function definition happening after main()
    cout << "This is my 1st C++ function" << endl;
};
